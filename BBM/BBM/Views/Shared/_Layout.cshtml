    <script type="text/javascript">
        var statusProducts = JSON.parse('@Html.Raw(BBM.Business.Infractstructure.EnumHelper<BBM.Business.Models.Enum.StatusProduct>.ToJson(typeof(BBM.Business.Models.Enum.StatusProduct)))');
        var lstStatusVAT = JSON.parse('@Html.Raw(BBM.Business.Infractstructure.EnumHelper<BBM.Business.Models.Enum.StatusVATProduct>.ToJson(typeof(BBM.Business.Models.Enum.StatusVATProduct)))');
        var statusOrder_Sale = JSON.parse('@Html.Raw(BBM.Business.Infractstructure.EnumHelper<BBM.Business.Models.Enum.StatusOrder_Sale>.ToJson(typeof(BBM.Business.Models.Enum.StatusOrder_Sale)))');
        var statusOrder_Input = JSON.parse('@Html.Raw(BBM.Business.Infractstructure.EnumHelper<BBM.Business.Models.Enum.StatusOrder_Input>.ToJson(typeof(BBM.Business.Models.Enum.StatusOrder_Input)))');
        var statusOrder_Output = JSON.parse('@Html.Raw(BBM.Business.Infractstructure.EnumHelper<BBM.Business.Models.Enum.StatusOrder_Output>.ToJson(typeof(BBM.Business.Models.Enum.StatusOrder_Output)))');
        var statusOrder_Suppliers = JSON.parse('@Html.Raw(BBM.Business.Infractstructure.EnumHelper<BBM.Business.Models.Enum.StatusOrder_Suppliers>.ToJson(typeof(BBM.Business.Models.Enum.StatusOrder_Suppliers)))');
        var statusProducts_Switch = JSON.parse('@Html.Raw(BBM.Business.Infractstructure.EnumHelper<BBM.Business.Models.Enum.StatusProductsSwitch>.ToJson(typeof(BBM.Business.Models.Enum.StatusProductsSwitch)))');


        var common = new Common.mvCommon();
        $(document).ready(function () {

            var currencypattern = "-n $;n $";
            Globalize.culture('vi-VN');
            Globalize.culture().numberFormat.currency.symbol = 'đ';
            Globalize.culture().numberFormat.currency.pattern = currencypattern.split(";");

            common.Start();
            $('#seletc_Channel').change(function () {
                var optionSelected = $(this).find("option:selected");
                var valueSelected = optionSelected.val();
                var textSelected = optionSelected.text();

            });
        });
    </script>
    @RenderSection("Scripts", required: false)
    <!-- Plugins -->
    @Html.Partial("~/Views/Shared/Partial/module/Partial/_template.cshtml");

    <div id="confirmModal" class="modal fade confirmModal" tabindex="-1" role="dialog" aria-labelledby="mySmallModalLabel" aria-hidden="true">
        <div class="modal-dialog ">
            <div class="modal-content">
                <div class="modal-header">
                    <h4 class="modal-title"></h4>
                </div>
                <div class="modal-body over-hide">
                    <p></p>
                </div>
                <div class="modal-footer mt10">
                    <button class="btn btn-default btnCancel">Hủy</button>
                    <button class="btn btn-primary btnOk">Yes</button>
                </div>
            </div>
        </div>
    </div>

